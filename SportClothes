package Item.SportClothes;

import Item.SportEquipment.*;

public class SportClothes extends SportEquipment {

	protected String sex;
	protected String material;
	protected String size;
	protected String color;

	public SportClothes() {
		// TODO - implement SportClothes.SportClothes
		throw new UnsupportedOperationException();
	}

	/**
	 * 
	 * @param sex
	 * @param material
	 * @param size
	 */
	public SportClothes(String equipName,double price,Producer producer,Imagine imagine,SellStore sellStore,String ID,double amountEquip,String sex, String material, String size,String color) {
		// TODO - implement SportClothes.SportClothes
		super(equipName, price, producer, imagine, sellStore, ID,amountEquip);
		this.sex=sex;
		this.material=material;
		this.size=size;
		this.color=color;
		throw new UnsupportedOperationException();
	}

	public String getSex() {
		return this.sex;
	}

	/**
	 * 
	 * @param sex
	 */
	public void setSex(String sex) {
		this.sex = sex;
	}

	public String getMaterial() {
		return this.material;
	}

	/**
	 * 
	 * @param material
	 */
	public void setMaterial(String material) {
		this.material = material;
	}

	public String getSize() {
		return this.size;
	}

	/**
	 * 
	 * @param size
	 */
	public void setSize(String size) {
		this.size = size;
	}

	public String getColor() {
		return this.color;
	}

	/**
	 * 
	 * @param color
	 */
	public void setColor(String color) {
		this.color = color;
	}

}
